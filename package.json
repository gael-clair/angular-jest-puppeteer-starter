{
  "name": "angular-jest-puppeteer-material-tailwindcss-starter",
  "version": "0.0.0",
  "description": "Angular + Jest + Puppeteer + Material + TailwindCSS starter project",
  "license": "MIT",
  "private": true,
  "scripts": {
    "init": "cross-var npx replace-in-file /angular-jest-puppeteer-material-tailwindcss-starter/gi \"$npm_package_name\" package.json,angular.json,src/app/app.component.ts,test/spec/src/app.component.spec.ts,test/e2e/src/app.e2e-spec.ts,src/index.html --isRegex",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "doc": "typedoc ./src",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:ci": "ng lint --format junit > ./reports/lint.xml",
    "lint:style": "stylelint \"src/app/**/*.scss\" \"src/styles.scss\"",
    "lint:style:fix": "stylelint \"src/app/**/*.scss\" \"src/styles.scss\" --fix",
    "lint:style:ci": "stylelint \"src/app/**/*.scss\" \"src/styles.scss\" --custom-formatter \"node_modules/stylelint-junit-formatter\" > ./reports/style.xml",
    "test": "jest --config ./test/spec/jest.config.js",
    "test:watch": "jest --config ./test/spec/jest.config.js --watch",
    "test:cov": "jest --config ./test/spec/jest.config.js --coverage ",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/spec/jest.config.js --runInBand",
    "test:ci": "jest --config ./test/spec/jest.config.js --ci --runInBand --coverage",
    "e2e": "jest --config ./test/e2e/jest.config.js",
    "e2e:start": "concurrently --success first -k -c \"green,yellow\" -n \"Serve,Tests\" \"ng serve\" \"wait-on http-get://localhost:4200/ && jest --config ./test/e2e/jest.config.js\"",
    "e2e:watch": "jest --config ./test/e2e/jest.config.js --watch",
    "e2e:cov": "concurrently --success first -k -c \"green,yellow\" -n \"Serve,Tests\" \"ng serve\" \"wait-on http-get://localhost:4200/ && jest --config ./test/e2e/jest.config.js --coverage && remap-istanbul --input ./coverage/e2e/coverage-puppeteer-istanbul.json --type html --output ./coverage/e2e/html-report\"",
    "e2e:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --config ./test/e2e/jest.config.js --runInBand",
    "e2e:ci": "concurrently --success first -k -c \"green,yellow\" -n \"Serve,Tests\" \"ng serve\" \"wait-on http-get://localhost:4200/ && jest --config ./test/e2e/jest.config.js --ci --coverage && remap-istanbul --input ./coverage/e2e/coverage-puppeteer-istanbul.json --type lcovonly --output ./coverage/e2e/lcov.info\""
  },
  "dependencies": {
    "@angular/animations": "~9.1.6",
    "@angular/cdk": "^9.2.3",
    "@angular/common": "~9.1.6",
    "@angular/compiler": "~9.1.6",
    "@angular/core": "~9.1.6",
    "@angular/forms": "~9.1.6",
    "@angular/material": "^9.2.3",
    "@angular/platform-browser": "~9.1.6",
    "@angular/platform-browser-dynamic": "~9.1.6",
    "@angular/router": "~9.1.6",
    "hammerjs": "^2.0.8",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^9.1.0",
    "@angular-devkit/build-angular": "~0.901.5",
    "@angular/cli": "~9.1.5",
    "@angular/compiler-cli": "~9.1.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@fullhuman/postcss-purgecss": "^2.2.0",
    "@types/expect-puppeteer": "^4.4.1",
    "@types/jest": "^25.2.1",
    "@types/jest-environment-puppeteer": "^4.3.1",
    "@types/node": "^12.11.1",
    "@types/puppeteer": "^2.0.1",
    "autoprefixer": "^9.7.6",
    "codelyzer": "^5.1.2",
    "commitizen": "^4.1.2",
    "concurrently": "^5.2.0",
    "cross-var": "^1.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "jest-preset-angular": "^8.2.0",
    "jest-puppeteer": "^4.4.0",
    "jest-puppeteer-istanbul": "^0.5.3",
    "lint-staged": "^10.2.2",
    "ngx-build-plus": "^9.0.6",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "prettier": "^2.0.5",
    "puppeteer": "^3.0.4",
    "remap-istanbul": "^0.13.0",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "stylelint-junit-formatter": "^0.2.2",
    "stylelint-no-unsupported-browser-features": "^4.0.0",
    "stylelint-prettier": "^1.1.2",
    "tailwindcss": "^1.4.6",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typedoc": "^0.17.6",
    "typedoc-hopper-theme": "^0.0.2",
    "typescript": "~3.8.3",
    "wait-on": "^5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
